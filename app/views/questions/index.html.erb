<h1>Chat</h1>

<%= render 'form', question: @question %>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Preguntas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% User.all.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.questions.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
